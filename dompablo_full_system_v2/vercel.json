{
  "version": 2,
  "builds": [
    {
      "src": "frontend/package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "frontend/dist" }
    }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "https://dompablo-backend.onrender.com/$1" },
    { "src": "/(.*)", "dest": "/frontend/dist/$1" }
  ]
}
